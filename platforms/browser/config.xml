<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.mygym" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>MyGym</name>
    <description>
        Aplicativo de gerenciamento e acessibilidade para academias.
    </description>
    <author email="brunozinek@outlook.com" href="https://www.facebook.com/bruno.zinek">
        My Gym
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
